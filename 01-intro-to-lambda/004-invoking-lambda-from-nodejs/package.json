{
  "name": "000-lambda-template-project",
  "version": "0.0.0",
  "description": "Project template for AWS Lambda function",
  "main": "index.js",
  "engines": {
    "node": "6.9.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sergiofgonzalez/all-things-lambda.git"
  },
  "keywords": [],
  "author": {
    "name": "Sergio F. Gonzalez",
    "email": "sergio.f.gonzalez@gmail.com"
  },
  "license": "ISC",
  "devDependencies": {},
  "dependencies": {
    "aws-sdk": "2.7.21"
  },
  "scripts": {
    "start": "node build/$npm_package_main",
    "lint": "eslint public/src/",
    "clean": "rm -rf build/",
    "build": "npm run lint",
    "prebuild": "npm run clean",
    "postbuild": "cp -r public/src/ build/",
    "debug": "node --inspect --debug-brk build/index.js",
    "dev": "nodemon --ext js,json,yml --watch public/src --watch .eslintrc.yml --exec 'npm run build:local && npm start'"
  }
}
